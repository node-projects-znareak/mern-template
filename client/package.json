  "scripts": {
    "dev": "vite --force --open --strictPort --host --port 4000",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint --fix .",
    "format": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "pre-commit": "npm run lint && npm run lint:fix && npm run format && npm run format:check",
    "prepare": "husky",
    "staged": "npx lint-staged --allow-empty",
    "husky:install": "npm run prepare && echo npm run staged > .husky/pre-commit"
  },
